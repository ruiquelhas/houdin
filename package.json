  "bugs": {
    "url": "https://github.com/ruiquelhas/houdin/issues"
  },
  "homepage": "https://github.com/ruiquelhas/houdin#readme",
  "devDependencies": {
    "code": "4.x.x",
    "coveralls": "2.x.x",
    "hapi": "15.x.x",
    "lab": "11.x.x",
    "multi-part": "2.x.x"
  },
  "dependencies": {
    "file-type": "3.x.x",
    "hoek": "4.x.x"
  },
  "engines": {
    "node": ">=4.5.0"
